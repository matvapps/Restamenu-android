<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StrokeTitleContainer">

        <attr name="stc_stroke_size" format="dimension" />
        <attr name="stc_hintText" format="string" />
        <attr name="stc_radius" format="float" />
        <attr name="stc_font_size" format="dimension" />
        <attr name="stc_stroke_color" format="color|reference" />
        <attr name="stc_text_color" format="color|reference" />


    </declare-styleable>

    <declare-styleable name="ServiceButton">
        <attr name="service_title" format="string" />
        <attr name="service_icon" format="color|reference"></attr>
    </declare-styleable>
    
    <declare-styleable name="SettingView">
        
    </declare-styleable>

    <declare-styleable name="NavMenuButton">
        <attr name="menu_title" format="string"/>
    </declare-styleable>

    <item name="match_parent" type="dimen">-1</item>
    <item name="wrap_content" type="dimen">-2</item>

    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="RestaurantsSearchView">
        <attr name="searchBackground" format="integer" />
        <attr name="searchVoiceIcon" format="integer" />
        <attr name="searchCloseIcon" format="integer" />
        <attr name="searchBackIcon" format="integer" />
        <attr name="searchSuggestionIcon" format="integer" />
        <attr name="searchSuggestionBackground" format="integer" />
        <attr name="android:hint" />
        <attr name="android:textColor" />
        <attr name="android:textColorHint" />
        <attr name="android:inputType" />
    </declare-styleable>

</resources>